{
    "name": "webgriffe/amp-magento-soap-client",
    "description": "An async Magento 1.x SOAP client to be used with Amphp framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Webgriffe SRL",
            "email": "support@webgriffe.com"
        }
    ],
    "require": {
        "clue/soap-react": "^0.2.0",
        "amphp/amp": "^2.0",
        "php": ">=7",
        "amphp/react-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {"Webgriffe\\AmpMagentoSoapClient\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Webgriffe\\AmpMagentoSoapClient\\": "tests/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "amphp/phpunit-util": "^1.0",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
